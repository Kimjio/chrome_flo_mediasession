{
  "manifest_version": 2,
  "name": "Flo MediaSession",
  "description": "Flo MediaSession Helper",
  "version": "1.0.0",
  "permissions": ["https://www.music-flo.com/"],
  "icons": {
    "96": "icon_96.png"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": ["content.js"],
      "matches": ["https://www.music-flo.com/*"],
      "run_at": "document_idle"
    }
  ]
}
